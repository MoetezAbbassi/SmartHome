    "version": "0.2.0",
    "configurations": [

        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "MainApp",
            "request": "launch",
            "mainClass": "smarthome.main.MainApp",
            "projectName": "SmartHome"
        },
        {
            "type": "java",
            "name": "SmartHomeGUI",
            "request": "launch",
            "mainClass": "smarthome.main.SmartHomeGUI",
            "projectName": "SmartHome"
        },
        {
            "type": "java",
            "name": "TestDevices",
            "request": "launch",
            "mainClass": "smarthome.main.TestDevices",
            "projectName": "SmartHome"
        },
        {
            "type": "java",
            "name": "TestHomeStructure",
            "request": "launch",
            "mainClass": "smarthome.main.TestHomeStructure",
            "projectName": "SmartHome"
        }
        {
            "type": "java",
            "name": "Launch SmartHomeGUI",
            "request": "launch",
            "mainClass": "smarthome.main.SmartHomeGUI",
            "vmArgs": "--module-path C:/javafx-sdk/lib --add-modules javafx.controls,javafx.fxml"
        }   
    ]
}